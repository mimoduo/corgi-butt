<!DOCTYPE html>
<html class="no-js" lang="en" itemscope="" itemtype="http://schema.org/WebPage">

	{# Shortcuts For Use Within Components #}
	{% set feat = "../components/feature/" %}
	{% set full = "../components/full-width/" %}
	{% set inc = "../components/in-content/" %}
	{% set side = "../components/sidebar/" %}
	{% set par = "../../partials/" %}

	{% set grid = false %}

	{% block page %}
		{% set page = {
			title: "Page",
			layout: "default"
		} %}
	{% endblock %}

	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="mobile-web-app-capable" content="yes">

		<title>{{ page.title }} | {{ vars.name }}</title>
		<meta name="description" content="{{ vars.description }}">

		{% include "../partials/favicons.html" %}

		<!-- Facebook -->
		<meta property="og:title" content="{{ page.title ?: vars.name }}">
		<meta property="og:type" content="website">
		<meta property="og:image" content="{{ page.metaImage ?: 'http://' ~ vars.domain ~ '/images/social-sq.png' }}">
		<meta property="og:image:width" content="{{ page.metaImageWidth ?: 800 }}">
		<meta property="og:image:height" content="{{ page.metaImageHeight ?: 800 }}">
		<meta property="og:description" content="{{ page.intro ?: vars.description }}">
		<meta property="og:site_name" content="{{ vars.name }}">

		{% if vars.twitter != "TwitterName" %}
		<!-- Twitter -->
		<meta name="twitter:card" content="{{ page.metaCard ?: 'summary' }}">
		<meta name="twitter:site" content="@{{ vars.twitter }}">
		<meta name="twitter:creator" content="@{{ vars.twitter }}">
		<meta name="twitter:url" content="//twitter.com/{{ vars.twitter }}">
		<meta name="twitter:title" content="{{ page.title ?: vars.name }}">
		<meta name="twitter:description" content="{{ page.intro ?: vars.description }}">
		<meta name="twitter:image" content="{{ page.metaImage ?: 'http://' ~ vars.domain ~ '/images/social-sq.png' }}">
		{% endif %}

		<script src="//code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>

		{% if page.stylesheets %}
		{% for styles in page.stylesheets %}
		<link rel="stylesheet" href="{{ styles }}">
		{% endfor %}
		{% endif %}

		<link rel="stylesheet" href="../../css/site.css?{{ 'now'|date('d-h') }}">
		<link href="https://fonts.googleapis.com/css?family=Permanent+Marker|Poppins:400,700" rel="stylesheet">
	</head>

	<body class="preload fs-grid page_layout_{{ page.layout }}{% if page.theme %} page_theme_{{ page.theme }}{% endif %}">

		{% block activePage %}
			{% set activePage = 0 %}
		{% endblock %}

		{% block mainNav %}
			{% set mainNavAlias = links.mainNav %}
		{% endblock %}

		{% block secondaryNav %}
			{% set secondaryNavAlias = links.secondaryNav %}
		{% endblock %}

		{% block breadcrumbNav %}
			{% set breadcrumbNav = [
				page.title
			] %}
		{% endblock %}

		{% block sectionTitle %}
			{% set sectionTitle = page.title %}
		{% endblock %}

		{% block subNav %}
			{% set subNavAlias = links.subNav %}
		{% endblock %}

		{% block utilityNav %}
			{% set utilityNavAlias = links.utilityNav %}
		{% endblock %}

		{% block quickNav %}
			{% set quickNavAlias = links.quickNav %}
		{% endblock %}

		{% block footerNav %}
			{% set footerNavAlias = links.footerNav %}
		{% endblock %}

		{% block socialNav %}
			{% set socialNavAlias = links.socialNav %}
		{% endblock %}

		{% include '../partials/update-notice.twig' %}

		{% import '../partials/macros.twig' as site %}

		<a class="skip_link" id="skip_link" href="#page_main_content">Skip to Main Content</a>

		<!-- Page Wrapper -->
		<div class="page_wrapper">
			<!-- Header -->
			<header class="header" id="header" itemscope itemtype="http://schema.org/WPHeader">
				<div class="header_ribbon">
					<div class="fs-row">
						<div class="fs-cell">
							<div class="header_ribbon_inner">
								<div class="header_ribbon_group">
									{% include "../partials/logo.twig" with {
										modifier: "header",
										icon: "logo"
									} %}

									{% include "../partials/navigation/main.twig" with {
										modifier: "lg",
										active: activePage,
										icon: "caret_down",
										links: mainNavAlias
									} %}
								</div>
								<div class="header_ribbon_group">
									<a class="header_email_link" href="mailto:mimoduo@gmail.com">
										<span class="header_email_link_inner">
											<span class="header_email_link_icon">{{ site.icon("mail") }}</span>
											<span class="header_email_link_label">Email Us</span>
										</span>
									</a>
									{% include "../partials/handle.twig" with {
										class: "menu",
										target: "menu",
										icon: "menu",
										label: "Open Menu"
									} %}
								</div>
							</div>
						</div>
					</div>
				</div>
			</header>
			<!-- END: Header -->

			<!-- Page -->
			<main class="page" itemprop="mainContentOfPage">
				{% if page.layout != "home" %}
				<!-- Page Feature -->
				<div class="page_feature">
					{% block page_feature %}
					{% endblock %}
				</div>
				<!-- END: Page Feature -->
				{% endif %}

				<!-- Page Content -->
				<div class="page_content">
					{% if page.layout != "home" %}
					<div class="fs-row">
						<!-- Main Content -->
						<div class="fs-cell{% if page.layout == "default" %} fs-lg-8{% else %} fs-lg-10 fs-all-justify-center{% endif %} page_main_content" id="page_main_content" tabindex="-1">
							<!-- Page Header -->
							{% block page_header %}
							<div class="page_header">
								{% if page.title %}
								<h1 class="page_title">
									<span class="page_title_icon" aria-hidden="true">{{ site.icon("paw") }}</span>
									<span class="page_title_label">{{ page.title }}</span>
								</h1>
								{% endif %}

								{% if page.intro %}
								<p class="page_intro">{{ page.intro }}</p>
								{% endif %}
							</div>
							{% endblock %}
							<!-- END: Page Header -->

							<!-- WYSIWYG -->
							<div class="typography">
								{% block typography %}
								{% endblock %}
							</div>
							<!-- END: WYSIWYG -->

							{% block after_typography %}
							{% endblock %}

							<!-- In Content Callouts -->
							<div class="in_content_callouts">
								{% block in_content_callouts %}
								{% endblock %}
							</div>
							<!-- END: In Content Callouts -->
						</div>
						<!-- END: Main Content -->

						{% if page.layout == "default" %}
						<!-- Sidebar Callouts -->
						<div class="fs-cell fs-lg-4 page_aside page_sidebar" itemscope itemtype="http://schema.org/WPSideBar">
							{% block sidebar %}
							{% endblock %}
						</div>
						<!-- END: Sidebar Callouts -->
						{% endif %}
					</div>
					{% endif %}

					<!-- Full Width Callouts -->
					<div class="full_width_callouts"{% if page.layout == "home" %} id="page_main_content"{% endif %}>
						{% block full_width_callouts %}
						{% endblock %}
					</div>
					<!-- END: Full Width Callouts -->
				</div>
				<!-- END: Page Content -->
			</main>
			<!-- END: Page -->

			<!-- Footer -->
			<footer class="footer" id="footer" itemscope itemtype="http://schema.org/WPFooter">
				<div class="footer_ribbon">
					<div class="fs-row">
						<div class="fs-cell">
							<div class="footer_ribbon_inner">
								{% include "../partials/meta.twig" with {
									name: vars.name,
									icon: "logo",
									state: "Maryland",
									email: "mimoduo@gmail.com",
									phone: "443-824-3742"
								} %}
							</div>
						</div>
					</div>
				</div>
			</footer>
			<!-- END: Footer -->
		</div>
		<!-- END: page_wrapper -->

		<!-- Menu -->
		<div class="js-menu menu" id="menu">
			{% include "../partials/navigation/main.twig" with {
				modifier: "sm",
				links: mainNavAlias,
				active: activePage,
				icon: "caret_down"
			} %}

			<a class="header_email_link" href="mailto:mimoduo@gmail.com">
				<span class="header_email_link_inner">
					<span class="header_email_link_icon">{{ site.icon("mail") }}</span>
					<span class="header_email_link_label">Email Us</span>
				</span>
			</a>

			<button class="js-swap js-menu-close menu_close" data-swap-target=".menu" data-swap-linked="menu" aria-label="Close Menu">
				<span class="menu_close_icon">{{ site.icon("close") }}</span>
				<span class="menu_close_label">Close Menu</span>
			</button>
		</div>
		<!-- END: Menu -->

		<script>
			var WWW_ROOT = "";
			var STATIC_ROOT = "../../";
		</script>
		{% if page.scripts %}
		{% for script in page.scripts %}
		{{ script }}
		{% endfor %}
		{% endif %}
		<script src="../../js/site.js?{{ 'now'|date('d-h') }}"></script>
		{% if grid %}
		<script>
			(function(){if(typeof FSGridBookmarklet==='undefined'){document.body.appendChild(document.createElement('script')).src='//formstone.it/js/bookmarklet/grid.js';}else{FSGridBookmarklet();}} )();
		</script>
		{% endif %}
	</body>
</html>
